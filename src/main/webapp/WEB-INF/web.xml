<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Project2</display-name>
  <servlet>
  	<servlet-name>GreetingServlet</servlet-name>
  	<servlet-class>csci310.servlets.GreetingServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>csci310.servlets.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<servlet-class>csci310.servlets.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStock</servlet-name>
  	<servlet-class>csci310.servlets.AddStock</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdatePrices</servlet-name>
  	<servlet-class>csci310.servlets.UpdatePrices</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RemoveStock</servlet-name>
  	<servlet-class>csci310.servlets.RemoveStock</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStockGraph</servlet-name>
  	<servlet-class>csci310.servlets.AddStockGraph</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ChangeDateGraph</servlet-name>
  	<servlet-class>csci310.servlets.ChangeDateGraph</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RemoveStock</servlet-name>
  	<url-pattern>/RemoveStock</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GreetingServlet</servlet-name>
  	<url-pattern>/greeting</url-pattern>
  	</servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdatePrices</servlet-name>
  	<url-pattern>/UpdatePrices</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  		<servlet-name>AddStock</servlet-name>
  		<url-pattern>/AddStock</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/Register</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  	<servlet-name>AddStockGraph</servlet-name>
  	<url-pattern>/AddStockGraph</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  	<servlet-name>ChangeDateGraph</servlet-name>
  	<url-pattern>/ChangeDateGraph</url-pattern>
  	</servlet-mapping>
</web-app>